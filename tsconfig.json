{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowJs": true,
    "strict": true,
    "noImplicitAny": true,
    "baseUrl": "src",
    "types": ["jest", "@testing-library/jest-dom","node"],
    "allowSyntheticDefaultImports": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}